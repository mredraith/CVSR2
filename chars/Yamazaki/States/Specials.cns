;Hebi Tsukai
[Statedef 1000]
type    = S
movetype =  A
physics = S
velset = 0,0
ctrl = 0
anim = 1000
poweradd = 72 * !var(20)

[State 1000, Voice]
type = PlaySnd
trigger1 = time = 7 || time = 150 || time = 300
value = 1000, ifelse((time = 7), 0, ifelse((time = 150), 1, 2))
channel = 0

[State 1000, VarSet]
type = VarSet
trigger1 = time = 0
fvar(30) = ifelse((command = "hebi_tsukai_x"), 1010, ifelse((command = "hebi_tsukai_z"), 1020, 1030))


[State 1000, 4]
type = ChangeState
trigger1 = command != "hold_x" && time >= 7 && fvar(30) = 1010
trigger2 = command != "hold_z" && time >= 7 && fvar(30) = 1020
trigger3 = command != "hold_y" && time >= 7 && fvar(30) = 1030
value = ceil(fvar(30))

[State 1000, 4]
type = ChangeState
trigger1 = time >= 7 && command = "c"
value = 1040

[State 1000, 4]
type = ChangeState
trigger1 = time = 400
value = 1001
