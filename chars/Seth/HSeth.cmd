TriggerAll = Var(59) < 1
TriggerAll = RoundState = 2
Trigger1 = AILevel > 0
v = 59
value = 1
Ignorehitpause = 1

[State -1, AI OFF]
Type = VarSet
Trigger1 = var(59) > 0
Trigger1 = RoundState != 2
Trigger2 = !IsHelper
Trigger2 = AILevel = 0
v = 59
value = 0
Ignorehitpause = 1

